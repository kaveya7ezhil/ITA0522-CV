import cv2
import numpy as np

img = cv2.imread("sample.jpg") # Ensure input.jpg exists in your folder

if img is not None:
    # A 5x5 kernel of ones defines the thickness of the operation
    kernel = np.ones((5, 5), np.uint8) 
    
    dilated = cv2.dilate(img, kernel, iterations=1)
    
    cv2.imshow("Original", img)
    cv2.imshow("Dilated", dilated)
    cv2.waitKey(0)
    cv2.destroyAllWindows()
